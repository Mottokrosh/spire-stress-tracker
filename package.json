        "cordova-plugin-hidden-statusbar": "^1.0.0",
        "cordova-plugin-whitelist": "^1.3.2",
        "cross-env": "^3.2.4",
        "laravel-mix": "^1.2.0",
        "vue": "^2.3.0",
